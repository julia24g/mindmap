{
  "name": "graphql-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "compile": "tsc",
    "start": "npm run compile && node ./dist/index.js",
    "test": "jest --testPathIgnorePatterns='dist/|tests/integration.test.ts'",
    "test:integration": "jest tests/integration.test.ts --runInBand --testTimeout=120000",
    "pgmigrate:up": "node-pg-migrate up --migrations-dir ../../database/migrations/postgres --database-url \"$DATABASE_URL\"",
    "pgmigrate:down": "node-pg-migrate down --migrations-dir ../../database/migrations/postgres --database-url \"$DATABASE_URL\"",
    "migrate": "npm run pgmigrate:up"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "type": "commonjs",
  "dependencies": {
    "@apollo/server": "^4.12.1",
    "@types/nock": "^10.0.3",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.5.0",
    "firebase": "^12.7.0",
    "firebase-admin": "^13.6.0",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.24.2",
    "jsonwebtoken": "^9.0.2",
    "neo4j-driver": "^5.28.1",
    "pg": "^8.16.0"
  },
  "devDependencies": {
    "@testcontainers/neo4j": "^11.7.1",
    "@testcontainers/postgresql": "^11.7.1",
    "@types/axios": "^0.9.36",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.15.19",
    "@types/pg": "^8.15.2",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "nock": "^14.0.5",
    "node-pg-migrate": "^8.0.4",
    "supertest": "^7.1.1",
    "testcontainers": "^11.7.1",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3"
  }
}
